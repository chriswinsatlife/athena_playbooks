---
import AthenaComBase from '../layouts/AthenaComBase.astro';
---

<style is:global>
  @media screen and (max-width: 991px) {
    .athena.nav[data-mobile-open='0'] .nav-links-container {
      display: none;
    }
    .athena.nav[data-mobile-open='1'] .nav-links-container {
      display: flex;
    }
    .athena.nav[data-mobile-open='0'] .trigger.hamburger {
      display: block;
    }
    .athena.nav[data-mobile-open='0'] .trigger.close {
      display: none;
    }
    .athena.nav[data-mobile-open='1'] .trigger.hamburger {
      display: none;
    }
    .athena.nav[data-mobile-open='1'] .trigger.close {
      display: block;
    }
  }
</style>

<AthenaComBase>
  <Fragment slot="head">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />
    <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js" is:inline></script>
  </Fragment>

  <div data-wf--navigation-bar-version-2--variant="base">
